<apex:page controller="candidateRegistrationCustom">
    <apex:form >
        <apex:pageBlock title="Candidate Registration" id="candidate_id">
            <apex:pageBlockSection columns="1">
            <apex:inputField value="{!ct.Salutation__c}"/>
                <apex:inputField value="{!ct.First_Name__c}"/>
                <apex:inputField value="{!ct.Last_Name__c}"/>
        <apex:inputField value="{!ct.DOB__c}"/>
        <apex:inputField value="{!ct.Email__c}"/>
<apex:inputField value="{!ct.Country__c}"/>
<apex:inputField value="{!ct.State__c}"/>
<apex:inputField value="{!ct.Age__c}"/>
<apex:inputField value="{!ct.Application_Date__c}"/>
<apex:inputField value="{!ct.Expected_Salary__c}"/>
<apex:inputField value="{!ct.phone_number__c}"/>
<apex:inputField value="{!ct.Status__c}"/>

            </apex:pageBlockSection>
            <apex:commandButton action="{!save }" value="Save" />
        </apex:pageBlock>
    </apex:form>
</apex:page>