<apex:page controller="JobRegistrationCustom">
    <apex:form >
        <apex:pageBlock title="Job Registration" id="candidate_id">
            <apex:pageBlockSection columns="1">
            <apex:inputField value="{!ct.Active__c}"/>
                <apex:inputField value="{!ct.Certification_Required__c}"/>
        <apex:inputField value="{!ct.Description__c}"/>
<apex:inputField value="{!ct.Expires_On__c}"/>
<apex:inputField value="{!ct.Manager__c}"/>
<apex:inputField value="{!ct.Number_of_Positions__c}"/>

<apex:inputField value="{!ct.phone__c}"/>

<apex:inputField value="{!ct.Required_Skills__c}"/>
<apex:inputField value="{!ct.Required_Skill__c}"/>
<apex:inputField value="{!ct.Salary_Offered__c}"/>

            </apex:pageBlockSection>
            <apex:commandButton action="{!save }" value="Save" />
        </apex:pageBlock>
    </apex:form>
</apex:page>